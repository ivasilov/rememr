{
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "semi": true,
  "useTabs": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "proseWrap": "always",
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
